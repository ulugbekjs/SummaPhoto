package Exnternals;
import Common.Photo;

public class PhotoObjectForClusteringExternals extends Photo {
	
	public boolean isVisited = false;
	public boolean isNoise = false; 
	public ClusterExternals cluster = null;
	
	
	public PhotoObjectForClusteringExternals(Photo p)
	{
		super(p.getTakenDate().toDate(), p.getWidth(), p.getHeight(), p.getLocation(), p.getFilePath());
	}
	
	public void addPointToCluster(ClusterExternals c)
	{
		if (c == null)
			return;
		this.cluster = c;
		c.photosInCluster.add(this);	
	}

}
