package Common;

import java.util.Date;
import java.util.Map;
import java.util.TreeMap;



public class EventContainerSingelton {

	private static final EventContainerSingelton instance = new EventContainerSingelton();
	private Map<Date,Event> events = new TreeMap<Date,Event>();

	private EventContainerSingelton() {
	}

	private void addNewEvent(Event event) {
		if (event != null) 
			events.put(event.getEventStartTime(), event);
	}
	
	private addPhotoToEvent() {
		
	}

	public static EventContainerSingelton getInstance() {
		return instance;
	}
}
