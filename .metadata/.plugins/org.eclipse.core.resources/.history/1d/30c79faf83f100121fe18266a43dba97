package Common;

import java.util.Date;
import java.util.Map;
import java.util.Set;
import java.util.TreeMap;
import java.util.TreeSet;



public class EventContainerSingelton {

	private static final EventContainerSingelton instance = new EventContainerSingelton();
	private Set<CollageEvent> events = new TreeSet<CollageEvent>();

	private EventContainerSingelton() {
	}

	private void addEventToContainer(CollageEvent event) {
		if ((event != null) && (events.contains(event)))
			events.add(event);
	}

	public static EventContainerSingelton getInstance() {
		return instance;
	}
}
